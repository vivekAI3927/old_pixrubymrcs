<section class="orange-fade p-5 margin-top-xl pos-r">
  <div class="container">
    <div class="row justify-content-center">
      <div class="col-lg-6 text-center">
        <div class="section-title">
          <h1 style="margin-top: 1%;">Resources</h1>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col-sm-12">
        <div class="pos-r">
          <div class="carousel-controls testimonial-carousel-controls">
            <div class="control prev"><i class="fa fa-chevron-left text-white">&nbsp;</i></div>
            <div class="control next"><i class="fa fa-chevron-right text-white">&nbsp;</i></div>
          </div>
          <div class="testimonial-carousel">
            <% Resource.all.where(available: true).each do |resource| %>
              <div class="white" style="min-height: 189px;">
                <a data-remote="true" href="/open_resource_modal?id=<%= resource.id%>" tabindex="0" >
                  <div class="testimonial w-100 h-100  p-3 text-center" style="min-height: 182px;">
                    <div>
                        <% if resource.image.present? %>
                          <%= image_tag resource.image.url, size: "50x40"%>
                        <% else %>
                          <i class="fa fa-3x fa-cloud" aria-hidden="true"></i>
                        <% end %> 
                    </div>
                    <br>
                    <div class="banner-content banner-text"><%= resource.title %></div>
                    <div class="banner-content banner-text"><%#= resource.description.html_safe %><%#= truncate(resource.description, :escape => false, length:100) %></div>
                  </div>
                </a>
              </div>
            <% end %>  
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
