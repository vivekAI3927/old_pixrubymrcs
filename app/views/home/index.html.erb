<!-- Start FAQ Area -->
<section class="faq orange-fade p-5 margin-top-xl pos-r">
  <div class="container">
  <div class="row">
    <h1 class="faq-title">Trial</h1>
    <div class="bs-example">
      <div class="accordion" id="accordionExample">
        <% @stations.each do |station| %>
          <div class="card">
            <div class="card-header" id="headingTwo">
              <h2 class="mb-0">
                <button type="button" class="btn btn-link" data-toggle="collapse" data-target="#collapseTwo<%= station.id%>"><%= station.title %> <i class="fa fa-plus"></i></button>
              </h2>
            </div>
            <div id="collapseTwo<%= station.id%>" class="collapse " aria-labelledby="headingTwo" data-parent="#accordionExample">
              <div class="card-body">
                <p> <%= station.scenario_text.html_safe %> </p>
                <%= link_to "show", station_path(station.id), :class => "join_btn trading-btn page-scroll" %>
              </div>
            </div>
          </div>
        <% end %>  
      </div>
    </div>
  </div>
</section>
<!-- End FAQ Area -->
