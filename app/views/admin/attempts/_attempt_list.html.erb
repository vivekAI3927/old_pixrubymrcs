<%@attempts.each_with_index do |x,index|%>
  <tr>
    <td width="5%"><%=index+1%></td>
    <td width="20%"><%=link_to x.user.try(:name), admin_user_path(x.user.id) rescue nil%></td>
    <td width="15%"><%=link_to x.station.title, admin_station_path(x.station.id) rescue nil%></td>
    <td width="20%"><%= x.started %></td>
    <td width="10%"><%= x.completed %></td>
    <td width="10%"> 
      <%=link_to admin_attempt_path(x) do%>
        <span class="glyphicon glyphicon-eye-open"></span>
      <%end%>
      <%= link_to edit_admin_attempt_path(x) do|| %>
        <span class="glyphicon glyphicon-pencil" style="color:#1ab394"></span>
      <% end %>
      <%= link_to admin_attempt_path(x), data: { confirm: 'Are you sure you want to delete Attempt '+ (x.id).to_s, "bootstrap-confirm": 'delete' }, method: :delete do|| %>
        <span class="glyphicon glyphicon-trash" style="color:red"></span>
      <% end %>
    </td>
  </tr>
<%end%>
