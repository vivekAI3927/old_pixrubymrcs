<%= render partial: "admin/breadcrumbs/breadcrumb", locals: {path: "member_feedbacks", title: "Member Feedback Page", current_page: "Member Feedback Page"} %>
<div class="content">
  <div class="row">
  </div>
  <br>
  <div class="row">
    <div class="col-sm-12">
      <div class="grid-view table-responsive" id="student-grid">
        <table class="items">
          <thead>
            <tr>
              <th id="student-grid_c0">Id</th>
              <th id="student-grid_c2">Description</th>
              <th class="button-column" id="student-grid_c7">Action</th>
            </tr>
          </thead>
          <tbody class="hole-list">
            <% @member_feedbacks.each_with_index do |x,index|%>
                <tr>
                  <td width="5%"><%= x.id %></td>
                  <td width="40%"><%= x.section_one.html_safe %><br/>
                    <%= x.section_two.try(:html_safe) %>
                  </td>
                  <td width="10%"> 
                    <%=link_to admin_member_feedback_path(x) do%>
                      <span class="glyphicon glyphicon-eye-open"></span>
                    <%end%>
                    <%= link_to edit_admin_member_feedback_path(x) do|| %>
                      <span class="glyphicon glyphicon-pencil" style="color:#1ab394"></span>
                    <% end %>
                  </td>
                </tr>
              <%end%>
          </tbody>
        </table>
        <%if @member_feedbacks.present?%>
          <div class="pager">

          </div>
        <%end%>
      </div>
    </div>
  </div>
</div>
