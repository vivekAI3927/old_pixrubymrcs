<%@parta_users.each_with_index do |x,index|%>
  <tr>
    <td width="10%"><%= link_to x.name, admin_parta_user_path(x) %></td>
    <td width="10%"><%=x.email%></td>
    <td width="10%"><%=x.country_name if x.country? %></td>
    <td width="15%"><%=x.created_at.strftime("%d-%m-%Y") rescue nil %></td>
    <td width="20%"><%=x.target_exam_date.strftime("%d-%m-%Y") rescue nil %></td>
    <td width="20%"><%= x.target_speciality.name rescue nil %></td>
    <td width="15%"> 
      <%if x.name.present?%>
        <%=link_to admin_parta_user_path(x) do%>
          <span class="glyphicon glyphicon-eye-open"></span>
        <%end%><br/>
        <%= link_to edit_admin_parta_user_path(x) do|| %>
          <span class="glyphicon glyphicon-pencil" style="color:#1ab394"></span>
        <% end %><br/>
        <%= link_to admin_parta_user_path(x), data: { confirm: 'Are you sure you want to delete this user', "bootstrap-confirm": 'delete' }, method: :delete do|| %>
         <span class="glyphicon glyphicon-trash" style="color:red"></span>
        <% end %>
      <%end%>
    </td>
  </tr>
<%end%>
