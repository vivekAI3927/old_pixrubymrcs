<%@faqs.each_with_index do |x,index|%>
  <tr>
    <td width="5%"><%= x.id %></td>
    <td width="25%"><%=link_to x.title, admin_faq_path(x) %></td>
    <td width="15%"><%= x.rank %></td>
    <td width="15%"><%= x.status %></td>
    <td width="10%"> 
      <%=link_to admin_faq_path(x) do%>
        <span class="glyphicon glyphicon-eye-open"></span>
      <%end%>
      <%= link_to edit_admin_faq_path(x) do|| %>
        <span class="glyphicon glyphicon-pencil" style="color:#1ab394"></span>
      <% end %>
      <%= link_to admin_faq_path(x), data: { confirm: 'Are you sure you want to delete this FAQ', "bootstrap-confirm": 'delete' }, method: :delete do|| %>
        <span class="glyphicon glyphicon-trash" style="color:red"></span>
      <% end %>
    </td>
  </tr>
<%end%>
