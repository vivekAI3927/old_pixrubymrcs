<%= render partial: "admin/breadcrumbs/breadcrumb", locals: {path: "parta_infos", title: "Part A Info", current_page: "Part A Info"} %>
<div class="content">
  <div class="row">
    <div class="col-sm-12">
      <div class="grid-view table-responsive" id="student-grid">
        <table class="items">
          <thead>
            <tr>
              <th id="student-grid_c0">Id</th>
							<th id="student-grid_c1">Area Tag</th>
              <th id="student-grid_c2">Heading/Subject</th>
              <th id="student-grid_c2">Body</th>
              <th id="student-grid_c1">Created At</th>
              <th id="student-grid_c1">Updated At</th>
              <th class="button-column" id="student-grid_c7">Action</th>
            </tr>
          </thead>
          <tbody class="hole-list">
            <%=render :partial=> "list"%>
          </tbody>
        </table>
        <%if @parta_info.present?%>
          <div class="pager">
          </div>
        <%end%>
      </div>
    </div>
  </div>
</div>
